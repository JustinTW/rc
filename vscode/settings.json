{
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "[dart]": {
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[vue]": {
    "editor.formatOnSave": false
  },
  "[go]": {
    "editor.formatOnSave": true
  },
  "[sql]": {
    // "editor.formatOnSave": true,
    "editor.defaultFormatter": "cymonk.sql-formatter"
  },
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "all",
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.df": "dockerfile",
    ".drone.yml": "yaml"
  },
  "files.trimFinalNewlines": false,
  "files.trimTrailingWhitespace": true,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": false,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "vue",
      "autoFix": true
    }
  ],
  "editor.formatOnType": false,
  "files.autoSave": "off",
  "editor.fontSize": 18,
  "files.useExperimentalFileWatcher": false,
  "python.jediEnabled": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[plsql]": {},
  "extensions.ignoreRecommendations": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "java.jdt.ls.vmargs": "-noverify -Xmx1G -XX:+UseG1GC -XX:+UseStringDeduplication -javaagent:\"/Users/robo/.vscode/extensions/gabrielbb.vscode-lombok-0.9.8/server/lombok.jar\"",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "explorer.confirmDelete": false,
  "python.linting.pylintEnabled": false,
  "yaml.schemas": {
    "file:///Users/justinct_liu/.vscode/extensions/docsmsft.docs-yaml-0.2.4/schemas/toc.schema.json": "/toc\\.yml/i"
  },
  "go.formatTool": "goimports",
  "python.languageServer": "Microsoft"
}